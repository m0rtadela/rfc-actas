<form action="" method="get">
    <div class="row">
        <div class="col-md-2 form-group">
            {{ filter_form.tipo.label(class="control-label") }}
            {{ filter_form.tipo(class="form-control") }}
        </div>
        <div class="col-md-2 form-group">
            {{ filter_form.state.label(class="control-label") }}
            {{ filter_form.state(class="form-control") }}
        </div>
        <div class="col-md-2 form-group">
            {{ filter_form.owner.label(class="control-label") }}
            {{ filter_form.owner(class="form-control") }}
        </div>
        <div class="col-md-2 form-group">
            {{ filter_form.start_date.label(class="control-label") }}
            {{ filter_form.start_date(class="form-control") }}
        </div>
        <div class="col-md-2 form-group">
            {{ filter_form.end_date.label(class="control-label") }}
            {{ filter_form.end_date(class="form-control") }}
        </div>
        <div class="col-md-2 form-group">
            {{ filter_form.order_by.label(class="control-label") }}
            {{ filter_form.order_by(class="form-control") }}
        </div>
