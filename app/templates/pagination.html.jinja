<nav aria-label="Page navigation" class="page-navigation">
    <ul class="pagination">
        {% if pagination.has_prev %}
            <li>
                <a href=" {{ url_for('sat.my_constancias', page=pagination.prev_num) }} " aria-label="Previous">
                    <span aria-hidden="true">&laquo;</span>
                </a>
            </li>
        {% endif %}
        {% for number in pagination.iter_pages() %}
            <li class={{ "active" if pagination.page == number else "" }}>
                <a href="{{ url_for('sat.my_constancias', page=number) if number else '#' }}">
                {{ number if number else '...' }}
                </a>
            </li>
        {% endfor %}
        
        {% if pagination.has_next %}
            <li>
                <a href="{{ url_for('sat.my_constancias', page=pagination.next_num) }}" aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                </a>
            </li>
        {% endif %}
    </ul>
</nav>